    </string-array>

    <!-- [Date Event Place Person], each byte means the number of each piece, and F means that the number is not 0 nor in the others number which exist in the list. -->
    <integer-array name="rule_ids" translatable="false">
        <item>0x1000</item>
        <item>0x1100</item>
        <item>0x1010</item>
        <item>0x1001</item>
        <item>0x1002</item>
        <item>0x1003</item>
        <item>0x100F</item>
        <item>0x1110</item>
        <item>0x1101</item>
        <item>0x1102</item>
        <item>0x1103</item>
        <item>0x110F</item>
        <item>0x1011</item>
        <item>0x1012</item>
        <item>0x1013</item>
        <item>0x101F</item>
        <item>0x1111</item>
        <item>0x1112</item>
        <item>0x1113</item>
        <item>0x111F</item>
    </integer-array>

    <!-- Each piece has three arraies, masks, fields and rules, and their lengths should be same. -->


    <!-- Date piece -->
    <integer-array name="date_rules_masks">
        <item>0x1000</item>
        <item>0x1000</item>
        <item>0x1FFF</item>
        <item>0x1FFF</item>
    </integer-array>

    <!-- Each byte means one of [NAMED-DATE] [UPPER-NAMED-DATE] [LOWER-NAMED-DATE] [DATE] [TIME] from upper to lower -->
    <integer-array name="date_rules_fields">
        <item>0x01011</item>
        <item>0x00010</item>
        <item>0x00111</item>
        <item>0x00010</item>
    </integer-array>

    <string-array name="date_rules">
        <item>[UPPER-NAMED-DATE] [TIME] ([DATE])</item>
        <item>[DATE]</item>
        <item>on [LOWER-NAMED-DATE] [TIME] ([DATE])</item>
        <item>on [DATE]</item>
    </string-array>

    <!-- Event piece -->
    <integer-array name="event_rules_masks">
        <item>0xF1FF</item>
    </integer-array>

    <!-- Each byte means one of [EVENT] from upper to lower -->
    <integer-array name="event_rules_fields">
        <item>0x1</item>
    </integer-array>

    <string-array name="event_rules">
        <item>[EVENT]</item>
    </string-array>

    <!-- Place piece -->
    <integer-array name="place_rules_masks">
        <item>0x1010</item>
        <item>0x1010</item>
        <item>0xFF1F</item>
        <item>0xFF1F</item>
    </integer-array>

    <!-- Each byte means one of [VENUS], [CITY] from upper to lower -->
    <integer-array name="place_rules_fields">
        <item>0x10</item>
        <item>0x01</item>
        <item>0x10</item>
        <item>0x01</item>
    </integer-array>

    <string-array name="place_rules">
        <item>At [VENUS]</item>
        <item>In [CITY]</item>
        <item>at [VENUS]</item>
        <item>in [CITY]</item>
    </string-array>

    <!-- Person piece -->
    <integer-array name="person_rules_masks">
        <item>0x1001</item>
        <item>0x1002</item>
        <item>0x1003</item>
        <item>0x100F</item>
        <item>0xFFF1</item>
        <item>0xFFF2</item>
        <item>0xFFF3</item>
        <item>0xFFFF</item>
    </integer-array>

    <!-- Each byte means one of [PERSON1], [PERSON2], [PERSON3] from upper to lower -->
    <integer-array name="person_rules_fields">
        <item>0x100</item>
        <item>0x110</item>
        <item>0x111</item>
        <item>0x111</item>
        <item>0x100</item>
        <item>0x110</item>
        <item>0x111</item>
        <item>0x111</item>
    </integer-array>

    <string-array name="person_rules">
        <item>With [PERSON1]</item>
        <item>With [PERSON1] and [PERSON2]</item>
        <item>With [PERSON1], [PERSON2] and [PERSON3]</item>
        <item>With [PERSON1], [PERSON2], [PERSON3] &#8230;</item>
        <item>With [PERSON1]</item>
        <item>with [PERSON1] and [PERSON2]</item>
        <item>with [PERSON1], [PERSON2] and [PERSON3]</item>
        <item>with [PERSON1], [PERSON2], [PERSON3] &#8230;</item>
    </string-array>

    <!-- Sentence Rules -->
    <integer-array name="sentence_rules_masks">
        <item>0x1000</item>
        <item>0x1100</item>
        <item>0x1010</item>

        <item>0x100F</item>

        <item>0x1110</item>
        <item>0x1101</item>
        <item>0x110F</item>

        <item>0x1011</item>
        <item>0x101F</item>

        <item>0x1111</item>
        <item>0x111F</item>
    </integer-array>

    <string-array name="sentence_rules">
        <item>[DATE-PIECE]</item>
        <item>[EVENT-PIECE] [DATE-PIECE]</item>
        <item>[PLACE-PIECE] [DATE-PIECE]</item>

        <item>[PERSON-PIECE] [DATE-PIECE]</item>

        <item>[EVENT-PIECE] [PLACE-PIECE] [DATE-PIECE]</item>
        <item>[PERSON-PIECE] [EVENT-PIECE] [DATE-PIECE]</item>
        <item>[EVENT-PIECE] [PERSON-PIECE] [DATE-PIECE]</item>

        <item>[PERSON-PIECE] [PLACE-PIECE] [DATE-PIECE]</item>
        <item>[PLACE-PIECE] [PERSON-PIECE] [DATE-PIECE]</item>

        <item>[PERSON-PIECE] [EVENT-PIECE] [PLACE-PIECE] [DATE-PIECE]</item>
        <item>[EVENT-PIECE] [PLACE-PIECE] [PERSON-PIECE] [DATE-PIECE]</item>
    </string-array>

</resources>
